{
  "name": "linesimulator",
  "version": "2.0.0",
  "description": "LINE Bot Simulator",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . LINESimulator --overwrite --platform=darwin --arch=x64 --icon=src/icons/favicon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . LINESimulator --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/icons/favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"LINE Simulator\"",
    "package-linux": "electron-packager . LINESimulator --overwrite --asar=true --platform=linux --arch=x64 --icon=src/icons/favicon.png --prune=true --out=release-builds"
  },
  "keywords": [
    "linebot"
  ],
  "author": "kenakamu",
  "license": "MIT",
  "devDependencies": {
    "appdmg": "^0.5.2",
    "browser-sync": "^2.24.6",
    "electron": "2.0.5",
    "electron-builder": "^20.26.0",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^12.1.0",
    "electron-winstaller": "^2.6.4",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-plumber": "^1.2.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^5.0.0-alpha.3",
    "source-map-support": "^0.5.6",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/jquery": "^3.3.4",
    "@types/nedb": "^1.8.5",
    "@types/socket.io": "^1.4.36",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.16.3",
    "nedb": "^1.8.0",
    "remote": "^0.2.6",
    "request": "^2.87.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  }
}
